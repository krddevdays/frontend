{
    "compilerOptions": {
        "allowJs": true,
        "jsx": "preserve",
        "lib": ["dom", "es2017"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "preserveConstEnums": true,
        "removeComments": false,
        "sourceMap": true,
        "strict": true,
        "target": "esnext"
    }
}
